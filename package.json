{
  "name": "kas-template",
  "private": true,
  "scripts": {
    "bootstrap": "npx lerna bootstrap",
    "build": "npm run bootstrap && npx lerna run build",
    "build:frontend": "cd ./packages/frontend && npm run build",
    "build:backend": "cd ./packages/backend && npm run build",
    "dev": "npm run bootstrap && (npm run dev:frontend & npm run dev:backend)",
    "dev:frontend": "cd ./packages/frontend && npm run dev",
    "dev:backend": "cd ./packages/backend && npm run dev",
    "test": "npx lerna run test",
    "start": "cd ./packages/backend && npm start",
    "postinstall": "npm run setup && npm run bootstrap",
    "setup": "node SETUP.js",
    "typesync": "npx typesync"
  },
  "devDependencies": {
    "@stencil/core": "^1.3.3",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.1.1",
    "@types/koa": "^2.0.52",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "bulma": "^0.8.0",
    "chalk": "^2.4.2",
    "cross-env": "^6.0.3",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lerna": "^3.18.4",
    "parcel": "^1.12.4",
    "restyped": "^1.1.0",
    "restyped-axios": "^2.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "stencil-quantum": "^0.2.0",
    "typescript": "3.6.4",
    "typesync": "^0.6.1"
  },
  "dependencies": {
    "uws": "10.148.1"
  }
}
